<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="8dp"
    android:layout_weight="1"
    app:cardElevation="2dp"
    app:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal"
            android:weightSum="10">


            <LinearLayout
                android:id="@+id/LinearLayoutMMUserProfile"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_weight="2.5">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="left">

                    <ImageView
                        android:id="@+id/ImageViewFeedbackProfile"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"


                        android:scaleType="centerCrop" />


                </FrameLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="1dp"
                android:layout_weight="7.5"
                android:orientation="vertical"

                >


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:gravity="center"
                    android:orientation="horizontal">
                    <!-- android:layout_height="@dimen/text_size_mymatches_desc"-->
                    <!--     <ImageView
                             android:id="@+id/ImageViewMMCountryFlag"
                             android:layout_width="18dp"
                             android:layout_height="@dimen/text_size_mymatches_desc"

                             android:layout_marginRight="3dp"
                             android:scaleType="fitXY"
                             android:visibility="visible"

                             />-->

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextVewFeedbackAlias"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="ADASASD"
                        android:textColor="@color/colorDashboardMainTextRed"
                        android:textSize="@dimen/text_size_mymatches_desc"
                        android:textStyle="bold" />

                    <!--      android:textColor="@color/colortextdashmymatches"-->
                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackAge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="3dp"
                        android:text="age"
                        android:textColor="#7c7c7c"
                        android:textSize="@dimen/text_size_mymatches_desc2" />

                    <ImageView
                        android:id="@+id/ImageViewMMViewPhone"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="6dp"

                        />
                    <!--@dimen/text_size_mymatches_desc-->

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPref1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="pref 1"
                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2" />

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPrefValue1"

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:gravity="center_vertical"
                    android:orientation="horizontal"

                    >

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPref2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2" />

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPrefValue2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPref3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2" />

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPrefValue3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2"
                        android:textStyle="bold" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPref4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2" />

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewFeedbackPrefValue4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textColor="@color/colortextdashmymatches"
                        android:textSize="@dimen/text_size_mymatches_desc2"
                        android:textStyle="bold" />


                </LinearLayout>


                <TextView
                    android:id="@+id/TextViewFeedbackAboutMe"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="2dp"
                    android:fontFamily="sans-serif"
                    android:text=""
                    android:textColor="@color/colortextdashmymatches"
                    android:textSize="@dimen/text_size_mymatches_desc2" />


            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"

            android:background="#ededed"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="left|center_vertical"
                android:visibility="invisible">


            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center_vertical">

                <android.support.v7.widget.LinearLayoutCompat
                    android:id="@+id/LinearLayoutFeedbackViewDetail"
                    style="@style/myCustomMaterialButton"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="15dp"
                    android:clickable="true">

                    <!--   <com.chicsol.marrymax.widgets.faTextView
                           android:id="@+id/faTextViewMyMatchesInterestIcon"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:layout_marginRight="6dp"
                           android:gravity="center"
                           android:text="@string/fa_icon_feedback"
                           android:textColor="@color/colorWhite"

                           android:textSize="@dimen/text_size_mymatches_desc" />-->

                    <com.chicsol.marrymax.widgets.mTextView
                        android:id="@+id/TextViewMMShowInterestText"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:gravity="center"
                        android:text=" View Detail "
                        android:textColor="#ffff"
                        android:textSize="@dimen/text_size_mymatches_desc" />

                </android.support.v7.widget.LinearLayoutCompat>


            </LinearLayout>


        </LinearLayout>
    </LinearLayout>


</android.support.v7.widget.CardView>